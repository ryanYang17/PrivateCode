<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <constant name="struts.devMode" value="true"/>

    <package name="student" extends="struts-default" namespace="/">

        <action name="addUser" class="me.codeboy.action.UserAction" method="addUser"/>
        <action name="addSignUpUser" class="me.codeboy.action.SignUpAction" method="addSignUpUser"/>
        <action name="addRegisterUser" class="me.codeboy.action.RegisterAction" method="addRegisterUser"/>
        <action name="addRegisterDriver" class="me.codeboy.action.RegisterAction" method="addRegisterDriver"/>
        <action name="addReserveOrder" class="me.codeboy.action.ReserveOrderAction" method="addReserveOrder"/>
        <action name="updateReserveOrderAfterReciveDriver" class="me.codeboy.action.ReserveOrderAction" method="updateReserveOrderAfterReciveDriver"/>
        <action name="updateReserveOrderAfterPaid" class="me.codeboy.action.ReserveOrderAction" method="updateReserveOrderAfterPaid"/>
    </package>

</struts>
